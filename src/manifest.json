{
  "name": "Yahoo Omnibox Search",
  "manifest_version": 2,
  "version": "1.0.0",
  "omnibox": { "keyword": "y" },
  "description": "This extension will enable searching yahoo search engine using google omnibox",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "yahoo16.png",
    "48": "yahoo48.png",
    "128": "yahoo128.png"
  },
  "permissions": ["https://search.yahoo.com/"]
}
